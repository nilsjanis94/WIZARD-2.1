[MASTER]
disable=C0114,C0115,C0116,C0301,C0103,W0613,W0622,R0903,R0913,R0914,R0915,W0703,E1101
max-line-length=100
extension-pkg-whitelist=PyQt6

[FORMAT]
max-line-length=100

[MESSAGES CONTROL]
disable=too-few-public-methods,
        too-many-arguments,
        too-many-instance-attributes,
        too-many-locals,
        too-many-branches,
        too-many-statements,
        line-too-long,
        missing-docstring,
        invalid-name,
        unused-argument,
        unused-variable,
        broad-except,
        bare-except,
        import-error

[BASIC]
good-names=i,j,k,ex,Run,_,id,pk,ip,db